tle={contentTxt.title}/>\r\n\r\n            <Zeitplan contentTxt={contentTxt}/>\r\n\r\n            <SubTitle title={contentTxt.anfahrtTitle}/>\r\n            <Anfahrt contentTxt={contentTxt}/>\r\n        </>\r\n    );\r\n}\r\n\r\nfunction LanguageBtn(props){\r\n    return(\r\n        <Link to={props.isGerman ? \"/russian\" : \"/gottesdienste\"} className=\"languageBtn-shell\">\r\n            <button className=\"languageBtn\">Lan</button>\r\n        </Link>\r\n    );\r\n}\r\n\r\nfunction Zeitplan(props){\r\n    return(\r\n        <div className=\"zeitplan-shell\">\r\n            <table className=\"zeitplan\">\r\n                <tbody>\r\n                    {props.contentTxt.terminTabelle.map((termin) => {\r\n                        return(\r\n                            <tr key={termin.tag + termin.uhrzeit}>\r\n                                <td>{termin.tag}</td>\r\n                                <td>{termin.uhrzeit}</td>\r\n                                <td>{termin.typ}</td>\r\n                            </tr>\r\n                        );\r\n                    })}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    );\r\n}\r\n\r\n\r\nfunction Anfahrt(props){\r\n    return(\r\n        <div className=\"anfahrtPanel\">\r\n            <div className=\"address-map-shell\">\r\n                <Map/>\r\n                <div className=\"addressPanel\">\r\n                    <h3 className=\"addressParagraph\">{props.contentTxt.adresse.title}</h3>\r\n                    <p className=\"addressParagraph\">{props.contentTxt.adresse.strasse}</p>\r\n                    <p className=\"addressParagraph\">{props.contentTxt.adresse.ort}</p>\r\n                    <a href=\"https://goo.gl/maps/XzEENGdEWXFMUCDt7\" target=\"_blank\">\r\n                        <p className=\"googleMapsBtn\">{props.contentTxt.mapsBtn}</p>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            <Wegbeschreibung contentTxt={props.contentTxt}/>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction Map(){\r\n    return(\r\n        <img className=\"map\" src=\"Sprites/map.png\" alt=\"map\"/>\r\n    );\r\n}\r\n\r\nfunction Wegbeschreibung(props){\r\n    return(\r\n        <div className=\"wegbeschreibungPanel\">\r\n            <h3 className=\"wegbeschreibungParagraph\">{props.contentTxt.wegbeschreibung.title}</h3>\r\n            {props.contentTxt.wegbeschreibung.absaetze.map((str) => {\r\n                return <p className=\"wegbeschreibungParagraph\" key={str}>{str}</p>\r\n            })}\r\n        </div>\r\n    )\r\n}","C:\\Users\\Christian Z\\Documents\\Gemeinde\\church-website\\src\\CookieBanners.js",["97"],[],"import { useState } from 'react';\r\nimport './CSS/CookieBanner.css';\r\n\r\nexport default function CookieBanner(props){\r\n    return(\r\n        <div className=\"banner-shell\">\r\n            <p className=\"banner-text\">By clicking “Accept All Cookies”, you agree to the storing of cookies on your device to enhance site navigation, analyze site usage, and assist in our marketing efforts.</p>\r\n            <div className=\"banner-buttons-shell\">\r\n                <BannerButton text=\"Ablehnen\" className=\"banner-button banner-button-reject\" setCookieConsent={props.setCookieConsent}/>\r\n                <BannerButton text=\"Annehmen\" className=\"banner-button banner-button-accept\" setCookieConsent={props.setCookieConsent}/>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction BannerButton(props){\r\n    return(\r\n        <button className={props.className} onClick={() => { localStorage.setItem('cookieConsent', props.text); props.setCookieConsent(props.text); }}>{props.text}</button>\r\n    );\r\n}","C:\\Users\\Christian Z\\Documents\\Gemeinde\\church-website\\src\\Pages\\Datenschutz.js",[],[],{"ruleId":"98","severity":1,"message":"99","line":30,"column":10,"nodeType":"100","messageId":"101","endLine":30,"endColumn":21},{"ruleId":"102","severity":1,"message":"103","lin